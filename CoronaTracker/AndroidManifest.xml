<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ankit.covidtracker">

    <application
        android:allowBackup="true"
        android:icon="@drawable/coronavirus_new"
        android:label="@string/app_name"
        android:roundIcon="@drawable/coronavirus_new"
        android:supportsRtl="true"
        android:theme="@style/Theme.CovidTracker">
        <activity android:name=".EachDistrictDataActivity"></activity>
        <activity android:name=".DistrictWiseDataActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AboutActivity" />
        <activity android:name=".EachCountryDataActivity" />
        <activity android:name=".CountryWiseDataActivity" />
        <activity android:name=".EachStateDataActivity" />
        <activity android:name=".StateWiseDataActivity" />
        <activity android:name=".WorldDataActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>